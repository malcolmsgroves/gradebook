<div>
  <tr>
    <th><strong>First Name</strong></th>
    <th><strong>Last Name</strong></th>
    <th><strong>Grade</strong></th>
  </tr>
  <%= @grades.each do |grade| %>
  <tr>
    <td>grade.student.first_name</td>
    <td>grade.student.last_name</td>
    <td>grade.grade</td>
  </tr>
  <% end %>
</div>
